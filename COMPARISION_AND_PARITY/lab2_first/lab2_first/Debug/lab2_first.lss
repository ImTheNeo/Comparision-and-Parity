000000 940c 0002                  jmp start ; jump over Interrupt Vectors, Program ID etc.
                                 
                                 ; **************************** Main Program Code ************************
                                 start:
                                 ; initialize the stack pointer to the highest RAM address
000002 ef0f                       ldi temp,low(RAMEND)
000003 bf0d                       out SPL,temp
000004 e100                       ldi temp,high(RAMEND)
000005 bf0e                       out SPH,temp
000006 e040                      ldi r20, ZEROS
000007 ef5f                      ldi r21, ONES
                                 ; configure the microprocessor pins for the control lines
000008 9abd                       sbi lcd_E_ddr, lcd_E_bit ; E line - output
000009 9aba                       sbi lcd_RS_ddr, lcd_RS_bit ; RS line - output
                                 ; initialize the LCD controller
00000a e000                      ldi r16, 0x00 ; Load 0 to R16
00000b bb0a                      out ddra, r16 ; Configure PORTA as input
00000c 940e 003f                 call lcd_init_4d ; initialize the LCD display for a 4-bit interface
                                 
                                 
                                 ; configure the microprocessor pins for the data lines
00000e 9aa7                       sbi lcd_D7_ddr, lcd_D7_bit ; 4 data lines - output
00000f 9aa6                       sbi lcd_D6_ddr, lcd_D6_bit
000010 9aa5                       sbi lcd_D5_ddr, lcd_D5_bit
000011 9aa4                       sbi lcd_D4_ddr, lcd_D4_bit
000012 bb4a                      HERE:out ddra, r20;a is input
000013 bb41                      out ddrd, r20;d is input
000014 b942                      out ddre, r20;e is input
000015 bb57                      out ddrb, r21; b is output
000016 bb54                      out ddrc, r21; c is output
000017 b369                      in r22,pina ;put the value in pina into r22
000018 b370                      	  in r23,pind;put the value in pind into r23
000019 b181                            in R24,pine;put the value in pine into r24
                                 
00001a 3080                      cpi R24, 0x00 ; check if the value entered from pine is 0
00001b f031                      breq call_odd_parity ; go to label
                                 
00001c 3081                      cpi R24, 0x01; check if the value entered from pine is 1
00001d f051                      breq call_even_parity; go to label
                                 
00001e 3082                      cpi R24, 0x02; check if the value entered from pine is 2
00001f f071                      breq call_show_bigger;go to label
                                 
000020 3083                      cpi R24, 0x03; check if the value entered from pine is 3
000021 f0a1                      breq call_show_diff; go to label
                                 
                                 
                                 
                                 
                                 call_odd_parity:
000022 940e 0650                 call display_empty_LineTwo;to be second line of lcd always empty
000024 940e 0300                 call odd_parity;call odd parity function
                                 
000026 940c 0012                 jmp HERE ; jump back to here
                                 
                                 
                                 call_even_parity:
000028 940e 0650                 call display_empty_LineTwo;to be second line of lcd always empty
00002a 940e 0360                 call even_parity;call even parity function
00002c 940c 0012                 jmp HERE ; jump back to here
                                 
                                 call_show_bigger:
00002e 940e 0400                 call show_bigger;call odd parity function
000030 940e 0700                 call display_empty_LineOne;to be first line of lcd always empty
000032 940e 0600                 call display2 ; call display in hexadecimal function
000034 940c 0012                 jmp HERE ; jump back to here
                                 
                                 call_show_diff:
000036 940e 0500                 call show_diff;call show diff parity function
000038 940e 0550                 call display1;call display in binary function
00003a 940e 0600                 call display2;call display in hexadecimal function
00003c 940c 0012                 jmp HERE ; jump back to here
                                 
                                 
                                 
                                 
                                 
                                 
